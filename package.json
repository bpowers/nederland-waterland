{
  "name": "nederland-waterland",
  "version": "0.0.1",
  "description": "Kunnen verzorgingstehuizen gebruikmaken van de oppervlaktewateren om energie in op te slaan?",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "clean": "rimraf ./public ./.cache ./dist",
    "cleanup": "npm run clean",
    "start": "parcel index.html",
    "build:domain": "npm run clean && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/nederland-waterland"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/nederland-waterland.git"
  },
  "keywords": [
    "vws",
    "tno",
    "nederland-waterland"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/nederland-waterland/issues"
  },
  "homepage": "https://github.com/erikvullings/nederland-waterland#readme",
  "dependencies": {
    "mithril": "^2.0.4",
    "leaflet": "^1.7.1",
    "osmtogeojson": "^3.0.0-beta.4",
    "osm-polygon-features": "^0.9.2",
    "@mapbox/geojson-rewind": "^0.5.0",
    "@turf/area": "^6.0.1",
    "@turf/centroid": "^6.0.2",
    "@turf/meta": "6.0.2",
    "@turf/helpers": "6.1.4"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.5.17",
    "@types/mithril": "^2.0.4",
    "@types/osmtogeojson": "^2.2.29",
    "cssnano": "^4.1.10",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  }
}